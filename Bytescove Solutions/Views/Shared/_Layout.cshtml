    <script>
        $('#services-slider').owlCarousel({
            margin: 20,
            loop: false,
            stagePadding: 100,
            nav: true,
            dots: false,
            responsive: {
                0: {
                    items: 1,
                    stagePadding: 20,
                    loop: true,
                },
                600: {
                    items: 2,
                    stagePadding: 30,
                },
                991: {
                    items: 3,
                    stagePadding: 50,
                },
                1024: {
                    items: 4
                }
            }
        });
        $('#ourteam-slider').owlCarousel({
            margin: 20,
            loop: false,
            nav: false,
            dots: false,
            responsive: {
                0: {
                    items: 1
                },
                480: {
                    items: 2
                },
                991: {
                    items: 3
                },
                1000: {
                    items: 4
                }
            }
        });
        $('#clients-slider').owlCarousel({
            margin: 20,
            loop: false,
            nav: false,
            dots: false,
            responsive: {
                0: {
                    items: 1
                },
                480: {
                    items: 2
                },
                600: {
                    items: 3
                },
                1000: {
                    items: 4
                }
            }
        });
        document.addEventListener('DOMContentLoaded', function () {
            const inputs = document.querySelectorAll('.contact-item .form-control');

            inputs.forEach(input => {
                const parent = input.closest('.contact-item');

                if (input.value) {
                    parent.classList.add('label-active');
                }

                input.addEventListener('focus', () => {
                    parent.classList.add('label-active');
                });

                input.addEventListener('blur', () => {
                    if (!input.value) {
                        parent.classList.remove('label-active');
                    }
                });
            });
        });
    </script>
